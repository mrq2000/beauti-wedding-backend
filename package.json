   "scripts": {
      "migrate:run": "yarn run build && typeorm migration:run",
      "migrate:rollback": "yarn run build && typeorm migration:revert",
      "migrate:gen": "yarn run build && typeorm migration:generate -n",
      "seed:run": "node ./node_modules/typeorm-seeding/dist/cli.js seed",
      "eslint": "eslint './src/**/*.ts'",
      "fix": "eslint --fix './src/**/*.ts'",
      "start": "rimraf dist && tsc-watch --onSuccess \"node --inspect ./dist/index.js\"",
      "build": "rimraf dist && tsc",
      "docker": "sudo docker-compose up -d",
      "prepare": "husky install"
